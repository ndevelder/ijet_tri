FoamFile
{
    version     2.0;
    format      ascii;
    class       volSymmTensorField;
    object      R;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
dimensions      [0 2 -2 0 0 0 0];
internalField   uniform (0 0 0 0 0 0);
boundaryField
{
	INLET
	{
        type            fixedValue;
        value           uniform (0 0 0 0 0 0);
	} 
	
	OUTLET_TOP
	{
        type            zeroGradient;
	}

	OUTLET_RIGHT
	{
        type            zeroGradient;
	}
	
    WALL_BOTTOM
    {
        type            fixedValue;
        value           uniform (0 0 0 0 0 0);
    }
	
    WALL_JET
    {
        type            zeroGradient;
    }
	
    ROT_AXIS{
		type	empty;
	}	
	
    ROT_WALL_FORE{
		type	wedge;
	}
	
	ROT_WALL_AFT{
		type	wedge;
	}
}
