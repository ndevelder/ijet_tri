FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      tpphi;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
dimensions      [0 0 0 0 0 0 0];
internalField   uniform 0.67;
boundaryField
{
	INLET
	{
		type groovyBC ;
		value $internalField;

		lookuptables (
		{
			name phidata;
			outOfBounds clamp;
			fileName "$FOAM_CASE/data/tpphi.csv";
		}
		);
        interpolate true;
		interpolationType cellPoint;
		valueExpression "phidata(pos().x)";
	} 
	
	OUTLET_TOP
	{
		type zeroGradient;
	}

	OUTLET_RIGHT
	{
		type zeroGradient;
	}
	
    WALL_BOTTOM
    {
        type            fixedValue;
        value           uniform 1e-10;
    }
	
    WALL_JET
    {
     	type            fixedValue;
        value           uniform 1e-10;
    }
	
    ROT_AXIS{
		type	empty;
	}		
	
    ROT_WALL_FORE{
		type	wedge;
	}
	
	ROT_WALL_AFT{
		type	wedge;
	}
}
// ************************************************************************* //
